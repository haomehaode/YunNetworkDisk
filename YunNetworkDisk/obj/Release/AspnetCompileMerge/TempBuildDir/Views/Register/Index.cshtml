@using System.Diagnostics;
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>注册</title>
    <link rel="stylesheet" type="text/css" href="css/register.css" />
</head>

<body>
    <!--头部-->
    <div class="top">
        <div style="float: left;">
            <table>
                <tr>
                    <td>&nbsp; &nbsp;&nbsp; &nbsp;<img src="assets/images/logo-icon.png" /> </td>
                    <td>
                        &nbsp; &nbsp;<font size="4"> 注册赟网盘账号</font>
                    </td>
                </tr>
            </table>
        </div>
        <div style="float:right;margin-top: 11px;">
            <font size="4"><a href="login.html">已有帐号，点击登录</a>&nbsp; &nbsp;&nbsp; &nbsp;</font>
        </div>
    </div>
    <div class="register">
        <!--表单-->
        <form method="POST">
            <table>
                <tr>
                    <td>&nbsp; &nbsp; 昵称 </td>
                    <td colspan="2">&nbsp; <input type="text" /></td>
                </tr>
                <tr>
                    <td>&nbsp; &nbsp; 手机 </td>
                    <td colspan="2">&nbsp; <input type="text" /></td>
                </tr>
                <tr>
                    <td>&nbsp; &nbsp; 密码 </td>
                    <td colspan="2">&nbsp; <input type="password" /></td>
                </tr>
                <tr>
                    <td>确认密码</td>
                    <td colspan="2">&nbsp; <input type="password" /></td>
                </tr>
                <tr>
                    <td>&nbsp; 验证码</td>
                    <td>&nbsp; <input type="text" id="code" /></td>
                    <td>
                        <input id="code" type="button" value="获取验证码" />
                    </td>
                </tr>
            </table>
            <input id="register_btn" type="button" value="注册" />
        </form>
    </div>
    <!--底部设计-->
    <div class="bottom">
        版权所有：“赟”网络科技有限公司<br>
        技术支持：郑州轻工业学院 官网：
        <a href="http://www.zzuli.edu.cn">www.zzuli.edu.cn</td>
    </div>

</body>

</html>